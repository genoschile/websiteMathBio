{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strictNullChecks": true,
    "allowJs": true
  },
  "baseUrl": ".",
  "paths": {
    "@/*": ["src/*"],
    "@components/*": ["src/components/*"],
    "@layouts/*": ["src/layouts/*"],
    "@pages/*": ["src/pages/*"],
    "@utils/*": ["src/utils/*"],
    "@assets/*": ["src/assets/*"]
  },
}
